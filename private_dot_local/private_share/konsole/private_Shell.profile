[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Monokai-Flat
Font=FiraCode Nerd Font Mono,11,-1,5,50,0,0,0,0,0
LineSpacing=0
UseFontLineChararacters=false

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=xterm-256color,SHLVL=0
Name=Shell
Parent=FALLBACK/
TerminalMargin=0
TerminalRows=39

[Scrolling]
HistorySize=10000

[Terminal Features]
BidiRenderingEnabled=true
BlinkingCursorEnabled=true
BlinkingTextEnabled=true
